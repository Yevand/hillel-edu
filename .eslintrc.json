{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true,
    "endOfLine": "auto",
    "bracketSpacing": false,
    "useTabs": true,
    "printWidth": 120
    }
}
